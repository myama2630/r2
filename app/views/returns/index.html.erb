<h1>Listing returns</h1>

<table>
  <thead>
    <tr>
      <th>Returndate</th>
      <th>Returncomment</th>
      <th>Sendingcomment</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @returns.each do | aReturn | %>
      <tr>
        <td><%= aReturn.returnDate %></td>
        <td><%= aReturn.returnComment %></td>
        <td><%= aReturn.sendingComment %></td>
        <td><%= link_to 'Show', aReturn %></td>
        <td><%= link_to 'Edit', edit_return_path(aReturn) %></td>
        <td><%= link_to 'Destroy', aReturn, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Return', new_return_path %>
